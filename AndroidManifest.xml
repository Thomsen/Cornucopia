<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csit.cornucopia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity android:name=".Cornucopia" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="Basic/Lifecycle/ActivityLifecycle"
            android:name="lifecycle.ActivityLifecycle" >

            <!-- android:configChanges="orientation|keyboardHidden" > -->
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="com.csit.cornucopia.CATEGORY_CORNUCOPIA" />
            </intent-filter>
        </activity>
        <activity
            android:label="Basic/Lifecycle/ServiceBindStart"
            android:name="lifecycle.ServiceBindStart" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="com.csit.cornucopia.CATEGORY_CORNUCOPIA" />
            </intent-filter>
        </activity>

        <service android:name="lifecycle.ServiceLifecycle" >
        </service>
    </application>

</manifest>